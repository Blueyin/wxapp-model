.section{
    width: 100%;
    position: relative;
}
.section input{
    width:630rpx;
    margin:5px auto;
    border:1px solid #c3c3c3;
    height:30px;
    border-radius: 3px;
    padding: 0 5px 0 50rpx;
    background:#fff;
}
.section icon{
    position: absolute;top:10rpx;
    left:40rpx;z-index: 100;
}
.text_box{
    padding: 10px 25px;background-color: #fff;
}
.text_title{font-size: 1em}
.text_info{font-size: .8em;color: #666}